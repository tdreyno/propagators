{
  "name": "@tdreyno/propagators",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "https://github.com/tdreyno/propagators.git",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "build": "rm -rf dist && tsc --project tsconfig.json",
    "compress": "terser pkg/dist-web/index.js -o pkg/dist-web/index.min.js --source-map --mangle --compress passes=2 --toplevel --mangle-props regex=/_$/ && gzip-size pkg/dist-web/index.min.js",
    "test": "jest",
    "test:inband": "jest --runInBand",
    "version": "npm run build",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint -c .eslintrc.js --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/prompt": "^17.7.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.61.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gzip-size-cli": "^5.1.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.1.0",
    "prettier": "^3.0.0",
    "semantic-release": "^22.0.5",
    "terser": "^5.19.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
