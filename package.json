{
  "name": "@tdreyno/propagators",
  "version": "1.0.0",
  "type": "module",
  "main": "build/index.js",
  "repository": "https://github.com/tdreyno/propagators.git",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "rm -rf dist && tsc --project tsconfig.json",
    "compress": "terser pkg/dist-web/index.js -o pkg/dist-web/index.min.js --source-map --mangle --compress passes=2 --toplevel --mangle-props regex=/_$/ && gzip-size pkg/dist-web/index.min.js",
    "test": "jest",
    "test:inband": "jest --runInBand",
    "version": "npm run build",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint . --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt": "^19.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.7",
    "@types/jest": "^29.5.12",
    "@eslint/js": "^9.6.0",
    "@types/eslint__js": "^8.42.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "gzip-size-cli": "^5.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "semantic-release": "^24.0.0",
    "terser": "^5.31.3",
    "ts-jest": "^29.1.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.24"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
